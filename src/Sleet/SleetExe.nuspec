<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SleetExe</id>
    <version>$version$</version>
    <authors>emgarten</authors>
    <owners>emgarten</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <icon>icon.png</icon>
    <projectUrl>https://github.com/emgarten/Sleet</projectUrl>
    <description>Sleet is a static feed generator for NuGet package feeds. This package contains Sleet.exe, an alternative to the dotnet tool.</description>
    <copyright>Copyright Â© 2021 Justin Emgarten</copyright>
    <tags>nuget nugetserver nugetfeed sleet</tags>
    <repository type="git" url="https://github.com/emgarten/Sleet" branch="$branch$" commit="$commit$" />
    <developmentDependency>true</developmentDependency>
    <icon>icon.png</icon>
  </metadata>
  <files>
    <file src="..\..\artifacts\publish\*.exe" target="tools\" />
    <file src="..\..\build\nupkgIncludes\Sleet.props" target="build\net46\Sleet.props" />
    <file src="..\..\build\common\icon.png" target="" />
  </files>
</package>
